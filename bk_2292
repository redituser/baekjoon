package bk_2292;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		
		int num = 1;
		int num1 = 0;
		int count = 0;
		
		while(n > num1) {
			
			num += 6;
			num1 += num;
			count++;
		}
		
		
		System.out.println(count+1);
		
		
	}

}
