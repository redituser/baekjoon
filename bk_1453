//소요시간 30분

package bk_1453;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count = 0;
		int idx = 0;
		boolean [] arr = new boolean[101]; // 좌석번호가 100까지 나오기때문
		
		for (int i = 0; i < n; i++) {
			idx = sc.nextInt();
			if(arr[idx]) {
				count ++;
			}else {
				arr[idx] =  true;
			}
			
			
		}	
		System.out.println(count);
		
		
		
		
		
	}

}
